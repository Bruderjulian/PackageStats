{
  "name": "packagestats",
  "version": "1.4.1",
  "description": "displays file/content Info about your package",
  "main": "index.js",
  "scripts": {
    "packageStats:scan": "node index.js scan",
    "packageStats:print": "node index.js print",
    "packageStats:inspect": "node --no-warnings index.js inspect",
    "packageStats:openViewer": "node index.js openViewer",
    "packageStats:closeViewer": "node index.js closeViewer",
    "packageStats:help": "node index.js help",
    "packageStats:version": "node index.js version",
    "packageStats:manageSaves": "node index.js manageSaves",
    "packageStats:configure": "node index.js configure",
    "test": "pta tests/**"
  },
  "author": "BruderJulian",
  "license": "MIT",
  "bin": {
    "packagestats": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bruderjulian/PackageStats.git"
  },
  "keywords": [
    "package",
    "viewer",
    "scanner",
    "packageStats",
    "stats",
    "fs",
    "project",
    "info",
    "tree",
    "filetree"
  ],
  "bugs": {
    "url": "https://github.com/Bruderjulian/PackageStats/issues"
  },
  "homepage": "https://github.com/Bruderjulian/PackageStats#readme",
  "dependencies": {
    "hjson-compressor": "^1.2.1",
    "path-validation": "^0.2.3",
    "text-extensions": "^2.4.0"
  },
  "devDependencies": {
    "pta": "^1.3.0",
    "zora": "^6.0.0"
  }
}
