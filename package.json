{
  "name": "packagestats",
  "version": "3.0.0",
  "description": "displays statistics about your package",
  "main": "index.js",
  "scripts": {
    "packageStats:version": "node index.js packageInfo --select=version",
    "packageStats:scan": "node index.js scan",
    "packageStats:print": "node index.js print",
    "packageStats:inspect": "node --no-warnings index.js inspect",
    "packageStats:view": "node index.js view",
    "packageStats:closeView": "node index.js closeView",
    "packageStats:cleanup": "node index.js cleanup",
    "packageStats:help": "node index.js help",
    "packageStats:packageInfo": "node index.js packageInfo",
    "commit": "git add . && git commit -m",
    "postcommit": "git push --all"
  },
  "author": "Julian",
  "license": "MIT",
  "bin": {
    "packagestats": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bruderjulian/PackageStats.git"
  },
  "keywords": [
    "package",
    "viewer",
    "scanner",
    "packageStats",
    "stats"
  ],
  "bugs": {
    "url": "https://github.com/Bruderjulian/PackageStats/issues"
  },
  "homepage": "https://github.com/Bruderjulian/PackageStats#readme",
  "dependencies": {
    "text-extensions": "^2.4.0"
  }
}
